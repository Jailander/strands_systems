<launch>
    <arg name="machine" default="strands-sidekick" />
    <arg name="user" default="hydro-default" />

    <machine name="$(arg machine)" address="$(arg machine)" env-loader="/opt/strands/strands_catkin_ws/devel/env.sh" user="$(arg user)" default="true"/>

    <!-- STRANDS people perception -->
    <include file="$(find perception_people_launch)/launch/people_tracker_robot.launch" >
        <arg name="machine"  value="$(arg machine)"/>
        <arg name="user"  value="$(arg user)"/>
        <arg name="ubd_config_file"  value="/home/hydro-default/catkin_ws/src/strands_perception_people/upper_body_detector/config/config_Asus.inp"/>
        <arg name="pt_config_file"  value="/home/hydro-default/catkin_ws/src/strands_perception_people/strands_pedestrian_tracking/config/config_Asus.inp"/>
        <arg name="template_file"  value="/home/hydro-default/catkin_ws/src/strands_perception_people/upper_body_detector/config/upper_temp_n.txt"/>
    </include>
</launch>
